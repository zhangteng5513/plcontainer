CREATE OR REPLACE FUNCTION rerror_timestamp(t timestamp) RETURNS timestamp AS $$
# container: plc_r_shared
options(digits.secs = 6)
tmp <- strptime(t,'%Y-%m-%d %H:%M:%OS')
return (as.character(tmp +i))
$$ LANGUAGE plcontainer;
